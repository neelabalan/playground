      "targets": [
        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    bucket_upper as le,\n    count\nFROM (\n    WITH duration_stats AS (\n        SELECT \n            MIN(total_duration) as min_duration,\n            MAX(total_duration) as max_duration\n        FROM builds\n        WHERE pipeline_name = '$pipeline'\n            AND total_duration IS NOT NULL  \n            AND $__timeFilter(build_start_time)\n    ),\n    buckets AS (\n        SELECT \n            generate_series(1, 5) as bucket_num,\n            min_duration,\n            max_duration,\n            (max_duration - min_duration) / 5.0 as bucket_size\n        FROM duration_stats\n    )\n    SELECT \n        CAST(min_duration + bucket_num * bucket_size AS DECIMAL(10,2)) as bucket_upper,\n        (\n            SELECT COUNT(*) \n            FROM builds \n            WHERE pipeline_name = '$pipeline'\n                AND total_duration IS NOT NULL\n                AND total_duration <= min_duration + bucket_num * bucket_size\n                AND $__timeFilter(build_start_time)\n        ) as count\n    FROM buckets\n) histogram_data\nORDER BY bucket_upper;",
          "refId": "A",
