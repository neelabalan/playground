          "format": "table",
          "rawQuery": true,
          "rawSql": "-- Queuing-time-distribution by bucket (seconds → minutes)\nSELECT\n    duration_range,\n    count\nFROM (\n    SELECT\n        CASE\n            WHEN queuing_time_seconds <= 60        THEN '0-1min'\n            WHEN queuing_time_seconds <= 120       THEN '1-2min'\n            WHEN queuing_time_seconds <= 240       THEN '2-4min'\n            WHEN queuing_time_seconds <= 480       THEN '4-8min'\n            WHEN queuing_time_seconds <= 960       THEN '8-16min'\n            WHEN queuing_time_seconds <= 1920      THEN '16-32min'\n            WHEN queuing_time_seconds <= 3840      THEN '32-64min'\n            WHEN queuing_time_seconds <= 7680      THEN '64-128min'\n            WHEN queuing_time_seconds <= 10800     THEN '128-180min'\n            ELSE                                    '180min+'\n        END AS duration_range,\n        COUNT(*) AS count\n    FROM (\n        SELECT\n            build_start_time,\n            COALESCE(blocked_time_seconds, 0)\n            + COALESCE(buildable_time_seconds, 0)\n            + COALESCE(waiting_time_seconds, 0) AS queuing_time_seconds\n        FROM builds\n        WHERE pipeline_name = '$pipeline'\n          AND build_start_time IS NOT NULL\n          AND $__timeFilter(build_start_time)\n    ) AS queuing_data\n    WHERE queuing_time_seconds IS NOT NULL\n    GROUP BY\n        CASE\n            WHEN queuing_time_seconds <= 60        THEN '0-1min'\n            WHEN queuing_time_seconds <= 120       THEN '1-2min'\n            WHEN queuing_time_seconds <= 240       THEN '2-4min'\n            WHEN queuing_time_seconds <= 480       THEN '4-8min'\n            WHEN queuing_time_seconds <= 960       THEN '8-16min'\n            WHEN queuing_time_seconds <= 1920      THEN '16-32min'\n            WHEN queuing_time_seconds <= 3840      THEN '32-64min'\n            WHEN queuing_time_seconds <= 7680      THEN '64-128min'\n            WHEN queuing_time_seconds <= 10800     THEN '128-180min'\n            ELSE                                    '180min+'\n        END\n) AS duration_dist\nORDER BY\n    CASE duration_range\n        WHEN '0-1min'     THEN 1\n        WHEN '1-2min'     THEN 2\n        WHEN '2-4min'     THEN 3\n        WHEN '4-8min'     THEN 4\n        WHEN '8-16min'    THEN 5\n        WHEN '16-32min'   THEN 6\n        WHEN '32-64min'   THEN 7\n        WHEN '64-128min'  THEN 8\n        WHEN '128-180min' THEN 9\n        WHEN '180min+'    THEN 10\n    END;\n",
